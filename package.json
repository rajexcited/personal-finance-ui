{
  "name": "ui-testing",
  "version": "1.0.0",
  "description": "Here, we are organizing test cases and relavant details. the all test cases are functional. the cosmetic issue fixes don't get recorded in test case, but screenshot will give away some idea on site UX.",
  "scripts": {
    "clean": "tsc --build --clean",
    "prebuild": "ts-node cypress/helpers/build-idb-injector.ts",
    "build": "tsc",
    "postbuild": "npm run clean",
    "pretests:regression:local": "npm run build",
    "tests:regression:local": "cross-env NODE_ENV=local cypress run --env grepTags=regression",
    "pretests:regression:dev": "npm run build",
    "tests:regression:dev": "cross-env NODE_ENV=development cypress run --env grepTags=regression",
    "pretests:regression:testplan": "npm run build",
    "tests:regression:testplan": "cross-env NODE_ENV=testplan cypress run --env grepTags=regression",
    "pretests:developer": "npm run build",
    "tests:developer": "cross-env NODE_ENV=local cypress open"
  },
  "license": "ISC",
  "homepage": "https://github.com/rajexcited/personal-finance-ui#readme",
  "devDependencies": {
    "@types/mochawesome": "^6.2.4",
    "@types/node": "^22.15.21",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@cypress/grep": "^4.1.0",
    "cypress": "^14.4.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "date-and-time": "^3.6.0",
    "dotenv": "^16.5.0",
    "dotenv-flow": "^4.1.0",
    "idb": "^8.0.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "uuid": "^11.1.0"
  }
}
